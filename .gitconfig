[user]
	name = Yamamoto Yusuke
	email = y.yamamoto88@gmail.com
[color]
	status = auto
	diff = auto
	interactive = auto
	grep = auto
	ui = true
[core]
	excludesfile = ~/.gitignore
	editor = vim
[alias]
	s = status
	st = status
	ss = status -s
	sh = show
	so = remote show origin
	po = push origin
  # commit
  ad  = add
  c   = commit
  ci  = commit -a
  cam = commit -a --amend
  co  = checkout
  cb  = checkout -b
  # branch
  br  = branch
  ba  = branch -a
  # log
  hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
[credential]
	helper = osxkeychain
